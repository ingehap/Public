while geolog.gettable():

    rhob_corr_cs = lfp_rhob * (1.0 - 0.1714 * (lfp_pef - pe_lith) * 0.01)
    sxo_cs = (lfp_rhomanc - lfp_rhog)/(rho_mf - lfp_rhog + c * (lfp_rhomanc - rhob_corr_cs)/(lfp_pef - pe_lith))
